        <td><img src="<%= song.thumbnail %>" class="thumbnail"</td>
        <td><%= song.name %></td>
        <td>
          <span style="cursor: pointer; display: inline-block;" onclick="doPlus('<%= song.id.to_s %>')">ğŸ‘</span>
          <%= song.poll %>
          <span style="cursor: pointer; display: inline-block;" onclick="doMinus('<%= song.id.to_s %>')">ğŸ‘</span>
        </td>
      </tr>
    <% end %>
    <script>
    emptyNewSongForm()
    sizeThumbnails()
    </script>
  </table>
</div>
